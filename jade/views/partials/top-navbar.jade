          .label.label-rejected {{ntc.notificationsCount}}

        ul.dropdown-menu(class="animated flipInX")
          li(ng-repeat="notification in ntc.flipNotification")
            .list-group
              //- a.list-group-item(ng-click="ntc.goNotification(notification); ntc.readNotification(notification)" ui-sref="app.mailbox.internal.in.view({mid: notification.document})")
              a.list-group-item(ng-if="notification.jobTitleID !== ntc.profile.jobTitle.id" ng-click="ntc.goNotification(notification); ntc.readNotification(notification)" )
                .media-box
                  .pull-left
                    em.fa.fa-envelope.fa-2x.text-warning
                  .media-box-body.clearfix
                      p.m0 {{notification.header}}
                      p.m0.text-muted : {{notification.message}}
              a.list-group-item(ng-if="notification.jobTitleID === ntc.profile.jobTitle.id" ng-click="ntc.goNotification(notification); ntc.readNotification(notification)" )
                .media-box
                  .pull-left
                    em.fa.fa-envelope.fa-2x.text-warning
                  .media-box-body.clearfix
                      p.m0 {{notification.header}}
                      p.m0.text-muted : {{notification.message}}
          li
